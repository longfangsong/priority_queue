<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cubeworkshop.priorityquque" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>优先队列</name>
  <description>
    An app to manage working items
  </description>
  <author email="longfangsong@icloud.com" href="http://github.com/longfangsong">
    LongFangsong
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <engine name="android" spec="^6.3.0"/>
  <engine name="ios" spec="^4.5.4"/>
  <plugin name="cordova-plugin-whitelist" spec="^1.3.3"/>
  <plugin name="cordova-plugin-vibration" spec="^2.1.6"/>
</widget>
